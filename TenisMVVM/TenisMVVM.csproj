<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <OutputType>WinExe</OutputType>
    <TargetFramework>net6.0-windows</TargetFramework>
    <Nullable>enable</Nullable>
    <UseWPF>true</UseWPF>
  </PropertyGroup>

  <ItemGroup>
    <None Remove="Assets\BebasNeue-Regular.ttf" />
    <None Remove="Assets\OleoScript-Bold.ttf" />
    <None Remove="Assets\OleoScript-Regular.ttf" />
    <None Remove="Assets\RobotoSlab-Regular.ttf" />
    <None Remove="Assets\RobotoSlab-VariableFont_wght.ttf" />
  </ItemGroup>

  <ItemGroup>
    <PackageReference Include="MvvmLightLibs" Version="5.4.1.1" />
    <PackageReference Include="Newtonsoft.Json" Version="13.0.1" />
  </ItemGroup>

  <ItemGroup>
    <Resource Include="Assets\BebasNeue-Regular.ttf" />
    <Resource Include="Assets\OleoScript-Bold.ttf" />
    <Resource Include="Assets\OleoScript-Regular.ttf" />
    <Resource Include="Assets\RobotoSlab-Regular.ttf" />
  </ItemGroup>

</Project>
